{
  "deviceName": "Vision",
  "fileName": "SystemManagement",
  "description": "System Management",
  "api": [
    {
      "label": "Get Management Info",
      "method": "GET",
      "basePath": "/mgmt/system/config/item/mngtinfo",
      "produces": "application/json",
      "description": "Retrieves Vision Management Info : macAddress, hostname ,defenseFlowId, defensePipeUrl, hardwarePlatform,activeServerMacAddress, serverSoftwareVersion, certificateHash"
    },
    {
      "label": "Add Site to TopologyTree",
      "method": "POST",
      "basePath": "/mgmt/system/config/tree/site",
      "produces": "application/json",
      "description": "Add New Site to TopologyTree"
    },
    {
      "label": "Set Authentication Mode",
      "method": "PUT",
      "basePath": "/mgmt/system/config/item/globalaaaparameters",
      "produces": "application/json",
      "description": "Set Authentication Mode"
    },
    {
      "label": "Get Management Info Ex",
      "method": "GET",
      "basePath": "/mgmt/system/config/item/mngtinfoex",
      "produces": "application/json",
      "description": "Retrieves Vision Management Info : macAddress, hostname ,defenseFlowId, defensePipeUrl, hardwarePlatform,activeServerMacAddress, serverSoftwareVersion, certificateHash, branch"
    },
    {
      "label": "Get Share Path State",
      "method": "GET",
      "basePath": "mgmt/system/config/itemlist/apm/sharepathstate",
      "produces": "application/json",
      "description": "Retrieves Vision Share path Info : An indication for APM status"
    },
    {
      "label": "Log Out",
      "method": "POST",
      "basePath": "/mgmt/system/user/logout",
      "produces": "application/json",
      "description": "Log Out from vision"
    },
    {
      "label": "Install License",
      "method": "POST",
      "basePath": "/mgmt/system/config/itemlist/visionlicense",
      "produces": "application/json",
      "consumes": "application/json",
      "description": "Install License , with following body fromat : {\"licenseStr\":\"%s\",\"requireDeviceLock\":true,\"parameters\": {}}"
    },
    {
      "label": "Delete License",
      "method": "DELETE",
      "basePath": "/mgmt/system/config/itemlist/visionlicense/{ormID}",
      "produces": "application/json",
      "description": "Delete License"
    },
    {
      "label": "Update Vision Navigation Xml",
      "method": "GET",
      "basePath": "/mgmt/system/config/dd/screen/navigation.xml",
      "produces": "application/json",
      "description": "Update Vision Navigation Xml"
    },
    {
      "label": "Set connectivity options",
      "method": "PUT",
      "basePath": "mgmt/system/config/item/connectionsettings",
      "produces": "application/json",
      "description": "Update connectivity options"
    }
  ]
}