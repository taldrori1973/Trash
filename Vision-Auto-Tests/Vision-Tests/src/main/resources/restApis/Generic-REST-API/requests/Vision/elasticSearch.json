{
  "deviceName": "Vision",
  "fileName": "elasticSearch",
  "description": "elasticsearch",
  "api": [
    {
      "label": "Delete Index",
      "method": "DELETE",
      "basePath": "/{indexName}",
      "produces": "application/json",
      "description": "delete index "
    },
    {
      "label": "Delete document by query",
      "method": "POST",
      "basePath": "/{indexName}/_delete_by_query?pretty=true",
      "produces": "application/json",
      "description": "delete document by query "
    },
    {
      "label": "Get document",
      "method": "GET",
      "basePath": "/{index}/_doc/{id}",
      "produces": "application/json",
      "description": "retrieve document "
    },
    {
      "label": "Get Index",
      "method": "GET",
      "basePath": "/{indexName}",
      "produces": "application/json",
      "description": "get index "
    },
    {
      "label": "Search index by query",
      "method": "POST",
      "basePath": "/{indexName}/_search/",
      "produces": "application/json",
      "description": "search index by query"
    },
    {
      "label": "Update index by query",
      "method": "POST",
      "basePath": "/{indexName}/_update_by_query/?conflicts=proceed&refresh",
      "produces": "application/json",
      "description": "update index by query"
    }
  ]
}

